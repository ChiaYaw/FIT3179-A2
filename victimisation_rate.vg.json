{
  "$schema": "https://vega.github.io/schema/vega-lite/v5.json",
  "title": "Victimisation Rate by State/Territory",
  "width": 800,
  "height": 600,
  "data": {
    "url": "https://raw.githubusercontent.com/ChiaYaw/week-9-homework/refs/heads/main/map.json",
    "format": {"type": "topojson", "feature": "ne_50m_admin_1_states_provinces"}
  },
  "transform": [
    {
      "lookup": "properties.NAME",
      "from": {
        "data": {
          "url": "https://raw.githubusercontent.com/ChiaYaw/week-9-homework/refs/heads/main/Victimisation_Rate.csv"
        },
        "key": "State",
        "fields": ["Victimisation_Rate"]
      }
    }
  ],
"projection": {
  "type": "albers",
  "parallels": [0, 0],
  "rotate": [0, 0]
  },
  "mark": {"type": "geoshape"},
  "encoding": {
    "color": {
      "field": "Victimisation_Rate",
      "type": "quantitative"
    },
    "tooltip": [
      {"field": "properties.NAME", "type": "nominal", "title": "State/Territory:"},
      {"field": "Victimisation_Rate", "type": "quantitative", "title": "Victimisation Rate:"}
    ]
  }
}
